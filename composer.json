{
    "name": "danial/simple-marketplace",
    "description": "A simple E-Commerce Package for Laravel.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^v3.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Danial\\SimpleMarketplace\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Danial Hatami"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Danial\\SimpleMarketplace\\Providers\\SimpleMarketplaceServiceProvider"
            ]
        }
    }

}
